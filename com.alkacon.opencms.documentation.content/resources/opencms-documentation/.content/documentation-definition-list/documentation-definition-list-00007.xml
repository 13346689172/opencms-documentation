  <DocumentationDefinitionList language="en">
    <Title><![CDATA[Sitemap Editor - Toolbar options]]></Title>
    <Heading><![CDATA[Available options]]></Heading>
    <PropertyDef>false</PropertyDef>
    <Entry>
      <Term><![CDATA[Publish]]></Term>
      <Icon>
        <link type="WEAK">
          <target><![CDATA[/sites/default/opencms-documentation/.galleries/icons_big_ade/publishBigIconActive.png]]></target>
          <uuid>7bb2098c-02bc-11e4-8167-1305b26fecac</uuid>
