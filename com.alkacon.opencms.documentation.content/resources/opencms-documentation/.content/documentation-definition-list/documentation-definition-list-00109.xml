<?xml version="1.0" encoding="UTF-8"?>

<DocumentationDefinitionLists xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="opencms://system/modules/com.alkacon.opencms.documentation/schemas/documentation-definition-list.xsd">
  <DocumentationDefinitionList language="en">
    <Title><![CDATA[Settings for export rules (sub-nodes of <export-rule>)]]></Title>
    <Heading><![CDATA[Settings for export rules (sub-nodes of <code>&lt;export-rule&gt;</code>)]]></Heading>
    <Anchor><![CDATA[overview_export_rules_options]]></Anchor>
    <PropertyDef>true</PropertyDef>
    <Entry>
      <Term><![CDATA[name]]></Term>
      <Description name="Description0">
        <links/>
        <content><![CDATA[<p>The name of the rule.</p>]]></content>
      </Description>
    </Entry>
    <Entry>
      <Term><![CDATA[description]]></Term>
      <Description name="Description0">
        <links/>
        <content><![CDATA[<p>A precise description of the uses of this rule.</p>]]></content>
      </Description>
    </Entry>
    <Entry>
      <Term><![CDATA[modified-resources]]></Term>
      <Description name="Description0">
        <links/>
        <content><![CDATA[<p>One or more regular expressions that specify a set of resources. If one of this resources becomes published, all resources specified by the <code>&lt;export-resources&gt;</code> node will be exported again.</p>]]></content>
      </Description>
    </Entry>
    <Entry>
      <Term><![CDATA[export-resources]]></Term>
      <Description name="Description0">
        <links/>
        <content><![CDATA[<p>One or more URIs (resources or folders), indicating a set of resources to be exported if any of the resources specified via the <code>&lt;modified-resources&gt;</code> node becomes published.</p>]]></content>
      </Description>
    </Entry>
  </DocumentationDefinitionList>
</DocumentationDefinitionLists>
