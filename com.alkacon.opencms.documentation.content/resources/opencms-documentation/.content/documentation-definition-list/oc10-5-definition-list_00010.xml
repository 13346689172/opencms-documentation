<?xml version="1.0" encoding="UTF-8"?>

<DocumentationDefinitionLists xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="opencms://system/modules/com.alkacon.opencms.documentation/schemas/documentation-definition-list.xsd">
  <DocumentationDefinitionList language="en">
    <Title><![CDATA[Properties of the CmsJspCategoryAccessBean]]></Title>
    <PropertyDef>true</PropertyDef>
    <Entry>
      <Term><![CDATA[categories.isEmpty]]></Term>
      <Description name="Description0">
        <links/>
        <content><![CDATA[<p>Yields <code>true</code> if no category is assigned at all, otherwise <code>false</code>.</p>]]></content>
      </Description>
    </Entry>
    <Entry>
      <Term><![CDATA[categories.subCategories["location/"]]]></Term>
      <Description name="Description0">
        <links>
          <link name="link0" internal="false" type="A">
            <target><![CDATA[http://documentation.opencms.org/javadoc/core/org/opencms/jsp/util/CmsJspCategoryAccessBean.html]]></target>
          </link>
        </links>
        <content><![CDATA[<p>Yields again a <code><a href="%(link0)">CmsJspCategoryAccessBean</a></code>&nbsp;for all sub-categories of <code>"location/"</code>.</p>]]></content>
      </Description>
    </Entry>
    <Entry>
      <Term><![CDATA[categories.allItems]]></Term>
      <Description name="Description0">
        <links>
          <link name="link0" internal="false" type="A">
            <target><![CDATA[http://documentation.opencms.org/javadoc/core/org/opencms/relations/CmsCategory.html]]></target>
          </link>
        </links>
        <content><![CDATA[<p>The list of all categories as <a href="%(link0)">CmsCategory</a>&nbsp;objects (sorted depth-first).</p>]]></content>
      </Description>
    </Entry>
    <Entry>
      <Term><![CDATA[categories.leafItems]]></Term>
      <Description name="Description0">
        <links>
          <link name="link0" internal="false" type="A">
            <target><![CDATA[http://documentation.opencms.org/javadoc/core/org/opencms/relations/CmsCategory.html]]></target>
          </link>
        </links>
        <content><![CDATA[<p>The list of all leaf categories (categories without sub-categories) as <code><a href="%(link0)">CmsCategory</a></code>&nbsp;objects.</p>]]></content>
      </Description>
    </Entry>
    <Entry>
      <Term><![CDATA[categories.topItems]]></Term>
      <Description name="Description0">
        <links>
          <link name="link0" internal="false" type="A">
            <target><![CDATA[http://documentation.opencms.org/javadoc/core/org/opencms/relations/CmsCategory.html]]></target>
          </link>
        </links>
        <content><![CDATA[<p>The list of all categories that are direct children of the current main category (as <code><a href="%(link0)">CmsCategory</a></code>&nbsp;objects).</p>]]></content>
      </Description>
    </Entry>
  </DocumentationDefinitionList>
</DocumentationDefinitionLists>
