<?xml version="1.0" encoding="UTF-8"?>

<FunctionConfigs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="opencms://system/modules/org.opencms.ade.containerpage/schemas/function-v2.xsd">
  <FunctionConfig language="en">
    <NiceName><![CDATA[Demo: Dynamic functions]]></NiceName>
    <Jsp>
      <link type="WEAK">
        <target><![CDATA[/system/modules/com.alkacon.opencms.documentation/functions/demo/time-example.jsp]]></target>
        <uuid>ffda275c-2129-11e4-a141-d9f74955ba1b</uuid>
      </link>
    </Jsp>
    <Description><![CDATA[A demo dynamic function. The configured provider shows time and date.]]></Description>
    <Match>
      <Types>
        <ContainerType><![CDATA[demo-content]]></ContainerType>
      </Types>
    </Match>
    <StrictContainers>true</StrictContainers>
    <Setting>
      <PropertyName><![CDATA[format]]></PropertyName>
      <DisplayName><![CDATA[Clock Format]]></DisplayName>
      <Widget><![CDATA[radio]]></Widget>
      <Default><![CDATA[both]]></Default>
      <WidgetConfig><![CDATA[time:Time|date:Date|both:Both]]></WidgetConfig>
    </Setting>
    <Setting>
      <PropertyName><![CDATA[style]]></PropertyName>
      <DisplayName><![CDATA[Date Style]]></DisplayName>
      <Widget><![CDATA[select]]></Widget>
      <Default><![CDATA[medium]]></Default>
      <WidgetConfig><![CDATA[short:Short|medium:Medium|long:Long|full:Full]]></WidgetConfig>
    </Setting>
    <Parameter>
      <Key><![CDATA[locale]]></Key>
      <Value><![CDATA[en_US]]></Value>
    </Parameter>
  </FunctionConfig>
</FunctionConfigs>
